set number
set numberwidth=3
set nocompatible
set backspace=indent,eol,start
set ruler
set incsearch
set smartindent
set autoindent
syntax on
filetype off
syntax enable
set background=dark
let g:solarized_termcolor = 256
colorscheme solarized
set tabstop=4
set softtabstop=4
set shiftwidth=4
set expandtab
